{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "max-len": "off",
    "camelcase": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "no-param-reassign": "off"
  }
}