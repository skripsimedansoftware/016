# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf

[*.{js, ts, tsx}]
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.{json, yml, yaml}]
indent_size = 2
indent_style = space
insert_final_newline = false